<Router AppAssembly="@typeof(App).Assembly">
    <Found Context="routeData">
        <RouteView RouteData="@routeData" DefaultLayout="@typeof(BlazorCms.Components.Layout.MainLayout)" />
        <FocusOnNavigate RouteData="@routeData" Selector="h1" />
    </Found>
    <NotFound>
        <PageTitle>Not found</PageTitle>
        <LayoutView Layout="@typeof(BlazorCms.Components.Layout.MainLayout)">
            <p role="alert">Désolé, il n'y a rien à cette adresse.</p>
        </LayoutView>
    </NotFound>
</Router>
